<launch>

<param name="/use_sim_time" value="true"/>

<node name="gp3" pkg="gp3" type="gp3_node">
    <param name="angular_velocity_lengthscale" value = "0.5"/>
    <param name="linear_acceleration_lengthscale" value="2"/>
    <param name="angular_velocity_noise_sd" value="0.05"/>
    <param name="linear_acceleration_noise_sd" value="0.5"/>

    <param name="max_data_points" value="20"/>
    <param name="prediction_time_offset" value="0.0"/>

    <remap from="input_imu" to="/jay/mavros/imu/data_raw"/>
</node>

</launch>