<launch>

<param name="/use_sim_time" value="true"/>

<node name="gp3" pkg="gp3" type="gp3_node">
    <param name="position_lengthscale" value = "0.3"/>
    <param name="orientation_lengthscale" value="0.3"/>
    <param name="position_signal_noise_sd" value="0.01"/>
    <param name="orientation_signal_noise_sd" value="0.01"/>
    <param name="max_data_points" value="50"/>
    <param name="prediction_rate_hz" value="100"/>
    <param name="prediction_time_offset" value="0.05"/>

    <remap from="input_transform" to="/vicon/auk/auk"/>
</node>

</launch>